{
  "kind": "singleType",
  "collectionName": "home_pages",
  "info": {
    "singularName": "home-page",
    "pluralName": "home-pages",
    "displayName": "Home Page",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string"
    },
    "Subtitle": {
      "type": "string"
    },
    "Image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "HomeLinks": {
      "type": "component",
      "repeatable": true,
      "component": "header.link-card",
      "max": 4,
      "min": 4
    },
    "Cards": {
      "type": "dynamiczone",
      "components": [
        "components.big-color-link-button",
        "components.html",
        "components.image-card",
        "components.label-link",
        "components.left-image-card",
        "components.media-link",
        "components.multimedia",
        "components.paragraph",
        "components.squares-grid",
        "components.title",
        "components.files-card"
      ]
    },
    "Video": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "videos"
      ]
    },
    "StartVideoAferTimeInMS": {
      "type": "integer",
      "required": false,
      "default": 700,
      "max": 60000,
      "min": 0
    }
  }
}
