{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "Post",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string"
    },
    "CreationDate": {
      "type": "date"
    },
    "LastUpdated": {
      "type": "datetime"
    },
    "Content": {
      "type": "dynamiczone",
      "components": [
        "components.big-color-link-button",
        "components.image-card",
        "components.left-image-card",
        "components.multimedia",
        "components.paragraph"
      ]
    },
    "Metadata": {
      "type": "component",
      "repeatable": false,
      "component": "metadata.metadata"
    },
    "Pretitle": {
      "type": "string"
    },
    "Subtitle": {
      "type": "text"
    },
    "URL_Name": {
      "type": "uid",
      "targetField": "Title"
    },
    "Video_or_Image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "videos"
      ]
    },
    "Author": {
      "type": "string"
    },
    "MainContent": {
      "type": "richtext"
    },
    "Category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::category.category"
    },
    "FullMedia": {
      "type": "boolean",
      "default": false,
      "required": true
    }
  }
}
