{
  "name": "ethf_web_page",
  "version": "1.0.0",
  "description": "Henry Ford Technical School's web page",
  "main": "index.js",
  "scripts": {
    "install": "cd frontend && npm install && cd ../backend && npm install",
    "start": "npx concurrently \"npm:backend\" \"npm:frontend\"",
    "dev": "npx concurrently \"npm:dev_backend\" \"npm:dev_frontend\"",
    "backend": "cd backend && npm start",
    "frontend": "cd frontend && npm start",
    "dev_backend": "cd backend && npm run develop",
    "dev_frontend": "cd frontend && npm run dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Martoxdlol/ethf-web-page.git"
  },
  "author": "Tom√°s Cichero <martoxdlol@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Martoxdlol/ethf-web-page/issues"
  },
  "homepage": "https://github.com/Martoxdlol/ethf-web-page#readme",
  "dependencies": {
    "concurrently": "^7.3.0"
  }
}
